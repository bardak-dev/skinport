{
  "name": "@skinport/backend",
  "description": "backend",
  "version": "0.0.0-development",
  "license": "GNU LGPL",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.1.4",
  "scripts": {
    "copyfiles": "copyfiles -u 1 src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json,csv} dist/",
    "pre:debug": "yarn copyfiles && yarn build:code",
    "start": "DOCKER_DEFAULT_PLATFORM=linux/amd64 docker compose up -d --build --remove-orphans && SWCRC=true node --trace-warnings --watch --experimental-detect-module -r \"dotenv/config\" --import @swc-node/register/esm-register src/index.ts",
    "build:code": "tsc",
    "build": "rm -rf dist build && yarn copyfiles && yarn build:code",
    "serve": "dotenv node dist/index.js",
    "build:docker": "docker buildx build --push --platform linux/amd64 -t bardakdev/ .",
    "dev:dist": "dotenv -- node dist/index.js"
  },
  "dependencies": {
    "@nestjs/common": "10.4.6",
    "@nestjs/core": "10.4.6",
    "@nestjs/platform-express": "10.4.6",
    "@nestjs/swagger": "7.4.2",
    "@typegoose/typegoose": "12.8.0",
    "bcrypt": "5.1.1",
    "chalk": "5.3.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "connect-redis": "7.1.1",
    "cookie": "1.0.1",
    "cookie-parser": "1.4.7",
    "copyfiles": "2.4.1",
    "cors": "2.8.5",
    "express": "4.21.1",
    "express-session": "1.18.1",
    "got": "^14.4.3",
    "ioredis": "5.4.1",
    "lodash.defaultsdeep": "4.6.1",
    "lodash.get": "4.4.2",
    "luxon": "3.5.0",
    "md5": "2.3.0",
    "mongodb": "6.10.0",
    "mongoose": "8.7.2",
    "mongoose-autopopulate": "1.1.0",
    "nestjs-ioredis-tags": "0.1.8",
    "nestjs-object-id": "2.0.1",
    "nestjs-typegoose": "7.1.38",
    "redis": "4.7.0",
    "swagger-ui-express": "5.0.1",
    "uid": "2.0.2"
  },
  "devDependencies": {
    "@swc-node/register": "1.10.9",
    "@swc/core": "1.7.39",
    "@types/bcrypt": "5.0.2",
    "@types/connect-redis": "0.0.23",
    "@types/cookie-parser": "1.4.7",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/express-session": "1.18.0",
    "@types/lodash.defaultsdeep": "4.6.9",
    "@types/lodash.get": "4.4.9",
    "@types/luxon": "3.4.2",
    "@types/md5": "2.3.5",
    "@types/node": "22.7.9",
    "dotenv": "16.4.5",
    "typescript": "5.6.3"
  }
}
